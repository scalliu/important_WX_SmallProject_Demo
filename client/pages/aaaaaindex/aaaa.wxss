page {
  background: black;
}

#topBanner   {
  width: 100%;
  height: 190rpx;
}

/*less sass*/

swiper image {
  width: 100%;
  height: 100%;
}

.demo-container {
  width: 50vw;
  height: 100vh;
  background: black;
}

/* 重置插件ui样式, 否则无法置顶无法显示2行 而且吸顶背景会有留白 */
.i-sticky-item-ab {
  height: auto !important;
}

.toggleWrap {
  background: black;
  font-size: 26rpx;
  color: white;
  text-align: center;
}

i-button-reset {
  flex: 1 1 33%;
}

.choiceWrap {
  width: 100vw;
  display: flex;
  height: 72rpx;
}


.choiceWrap .px1px {
  height: 35rpx;
  width: 1px;
  background: #3a3a3a;
  background-clip: content-box;
  margin-top: 19rpx;
  float: right;
}


.listWrapFather {
  width: 100vw;
}

.toggleWrap .toggle-icon {
  width: 30rpx;
  margin-left: 20rpx;
  height: 18rpx;
  display: inline-block;
  background-size: cover;
  background-position: center center no-repeat;
}

 

.dataWrap {  
  color: hsl(0, 0%, 46%);
  padding-left:20rpx;
  height: 68rpx;
  padding-right: 20rpx; 
  border-top:1px solid #3a3a3a; 
}

.dataWrap view {
  width:72rpx;  
  position: relative;
  font-size: 24rpx;
  float: left;
  /* border:1px solid blue; */
  text-align: center;
  /* word-break:; */
}


.dataWrap .split1Px{
  height:29rpx ;
  margin-top:9px; 
  width: 1px;
  flex:0 0 1px; 
  background: #3a3a3a; 
}

.dataWrap  .small10rpx{
  font-size: 20rpx; 
}

.hiden{
  display: none;
}

.dataWrap .currentData{
  padding-top:6rpx;
  line-height: 25rpx!important;
}

.dataWrap .currentData:after{
  content:'';
  border:2px solid red;
  width:80rpx;height: 80rpx;
  border-radius: 50%;
  position: absolute;
  top:-10rpx;
  left:-5rpx; 
  text-align: center;
  
}

.dataWrap  .teacherstyle{
  width:130rpx ;
}

.dataWrap  .teacherstyle::before{
  content:'';
  height:30rpx ;
  position: absolute;
  top: 18rpx;
  left: 6rpx; 
  width:1px; 
  background: #3a3a3a; 
  /* background: red;  */


}
.dataWrap  .CAPMstyle{
  float: right; width:70rpx ;
}




.drawer-header {
  font-size: 30rpx;
  color: white;
  background: #2d3030;
  height: 7.5vh;
  line-height: 7.5vh;
}

.drawer-icon {
  margin-left: 22rpx;
  margin-right: 15rpx;
  background-size: 100%;
  background-position: center;
  width: 44rpx;
  display: inline-block;
  height: 44rpx;
  vertical-align: middle;
}

.drawer-left view,
.drawer-right view {
  text-align: center;
  font-size: 26rpx;
  height: 96rpx;
  line-height: 96rpx;
}

.drawer-left {
  height: 92.5vh;
  float: left;
  width: 35%;
  background: #262929;
  color: #8a8e92;
}

.drawer-left view.active {
  color: #ffc800;
}

.drawer-right {
  height: 92.5vh;
  float: right;
  width: 65%;
  background: #383e3e;
  color: #ced3d4;
}

.drawer-right view text {
}

.drawer-right text.active {
  background: #ffc800;
  border-radius: 8rpx;
  color: black;
  display: inline-table;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0rpx 40rpx;
}

/* 选择右侧导航栏导航 */

.demo-container-right {
  background: #383e3e;
}

.drawer-left-right {
  margin-top: 30rpx;
}

.drawer-left-right view {
  margin-bottom: 56rpx;
}

.drawer-left-right view text {
  border: 1px solid white;
  display: block;
  margin: 0 30rpx;
  font-size: 26rpx;
  text-align: center;
  color: white;
  border-radius: 6px;
  height: 50rpx;
  line-height: 50rpx;
}

.drawer-left-right view text.Check {
}

.drawer-left-right view text.CheckS {
  background: #ffc800;
  color: black;
  border: 1px solid transparent;
}

/* 列表栏目 */

.listWrapFather {
  min-height: calc(100vh - 190rpx - 85rpx);
  height: calc(100vh - 190rpx - 85rpx);
  background: #232323;
}

#contentSwiper{
  height: 100%;
}

.shoppingName {
  padding: 0 18rpx;
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: white;
}

.shoppingName .mapTitle {
  color: #ccc;
  float: right;
}

.listWrap {
  background: #232323;
  color: #f6f6f3;
  margin: 0 auto;  
}

.listWrapFather .listWrap:last-of-type  .list-items {
  /* padding-bottom:  15rpx  ; */
  border:0;
}


.list-items {  
  overflow: hidden;
  border-bottom:1px solid #ccc ; 
  padding:20rpx 20rpx 25rpx  0rpx;
  margin-left: 20rpx
}

.list-items .avatar,
.list-items .list-items-center {
  float: left;
}

.list-items .list-items-right {
  float: right; 
}
.list-items .avatar {
  width: 150rpx;
  height: 150rpx; 
  border-radius: 50%;
}

.list-items .list-items-center {
  width: 400rpx;
  height: 150rpx;
  margin-left: 30rpx;
}

.list-items .list-items-center > view {
  margin-bottom: 25rpx;
}

.list-items-center .h1Title {
  font-size: 28rpx;
}

.list-items-center .h2Title {
  font-size: 20rpx;
  color: #eee;
}

.list-items-center .h3Title {
  font-size: 24rpx;
}

.h3Title .time {
  color: #fcc800;
  margin-right: 10rpx;
}

.h3Title .price {
  color: #b8b8b8;
}

.list-items-right{
   margin-top:70rpx;
}

.list-items-right .state {
    padding: 16rpx 20rpx;
  font-size: 26rpx;
  color: #ccc;
  border: 1px solid #ccc;
}
